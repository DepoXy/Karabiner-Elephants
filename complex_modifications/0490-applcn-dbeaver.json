{
  "title": "#0490 — DBeaver (org.jkiss.dbeaver.core.product) — Foregrounder",
  "rules": [

    {
      "description": "#0491 — DBeaver — Systemwide — Foreground (Shift-Ctrl-Cmd-E)",
      "manipulators": [
        {
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_control",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "osascript -e 'tell application \"DBeaver Community\" to reopen' -e 'tell application \"DBeaver Community\" to activate'"
            }
          ],
          "type": "basic"
        }
      ]
    }

  ]
}

