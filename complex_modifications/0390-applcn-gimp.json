{
  "title": "#0390 — Gimp — Remap Commands (b/c setting System Prefs Keyb Shortcuts does nothing!)",
  "rules": [

    {
      "description": "#0391 — Gimp — Remap GIMP-2.10 > Quit GIMP-2.10: Control-Q → Command-Q",
      "manipulators": [
        {
          "from": {
            "key_code": "q",
            "modifiers": { "mandatory": [ "left_control" ] }
          },
          "to": {
            "key_code": "q",
            "modifiers": [ "left_command" ]
          },
          "conditions": [{
            "type": "frontmost_application_if",
            "bundle_identifiers": [ "^org\\.gimp\\.gimp-2.10" ]
          }],
          "type": "basic"
        }
      ]
    },

    {
      "description": "#0392 — Gimp — Remap File > New...: Control-N → Command-N",
      "manipulators": [
        {
          "from": {
            "key_code": "n",
